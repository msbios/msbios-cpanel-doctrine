{
  "name": "msbios/cpanel-doctrine",
  "description": "MSBios CPanel Doctrine",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-cpanel-doctrine",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\CPanel\\Doctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\CPanel\\Doctrine\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-modulemanager": "^2.8",
    "zendframework/zend-mvc-plugins": "^1.0",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
    "msbios/msbios": "^0.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-test": "^3.1",
    "squizlabs/php_codesniffer": "^3.0",
    "msbios/application": "dev-master",
    "msbios/doctrine": "dev-master",
    "msbios/authentication": "dev-master",
    "msbios/guard-resource": "dev-master",
    "msbios/guard": "dev-master",
    "msbios/guard-cpanel": "dev-master",
    "msbios/guard-cpanel-doctrine": "dev-master",
    "msbios/resource": "dev-master",
    "msbios/resource-doctrine": "dev-master",
    "msbios/cpanel": "dev-master",
    "msbios/debug": "dev-master"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../msbios-application/"
    }, {
      "type": "path",
      "url": "../msbios-doctrine/"
    }, {
      "type": "path",
      "url": "../msbios-authentication/"
    }, {
      "type": "path",
      "url": "../msbios-guard-resource/"
    }, {
      "type": "path",
      "url": "../msbios-guard/"
    }, {
      "type": "path",
      "url": "../msbios-guard-cpanel/"
    }, {
      "type": "path",
      "url": "../msbios-guard-cpanel-doctrine/"
    }, {
      "type": "path",
      "url": "../msbios-resource/"
    }, {
      "type": "path",
      "url": "../msbios-resource-doctrine/"
    }, {
      "type": "path",
      "url": "../msbios-cpanel/"
    }, {
      "type": "path",
      "url": "../msbios-debug/"
    }
  ],
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit"
    ],
    "cleanup": "rm ./data/cache/*.php && rm ./data/logs/*.log",
    "alter": "./vendor/bin/doctrine-module orm:schema-tool:update --force",
    "assets": "cd bin && ln -s ../vendor/msbios/cpanel/themes/limitless/public/ limitless",
    "serve": "php -S 0.0.0.0:3107 -t bin bin/msbios-cpanel-doctrine-module.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  }
}
