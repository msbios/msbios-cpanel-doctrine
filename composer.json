{
  "name": "msbios/cpanel-doctrine",
  "description": "MSBios CPanel Doctrine",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-cpanel-doctrine",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\CPanel\\Doctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\CPanel\\Doctrine\\": "tests/"
    }
  },
  "require-dev": {
    "msbios/application": "^1.0",
    "msbios/resource": "^1.0",
    "msbios/authentication": "^1.0",
    "msbios/guard": "^1.0",
    "msbios/guard-resource": "^1.0",
    "msbios/guard-cpanel": "^1.0",
    "msbios/cpanel": "^1.0",
    "msbios/doctrine": "^1.0",
    "msbios/resource-doctrine": "^1.0",
    "msbios/authentication-doctrine": "^1.0",
    "msbios/guard-doctrine": "^1.0",
    "msbios/guard-resource-doctrine": "^1.0",
    "msbios/guard-cpanel-doctrine": "^1.0",
    "msbios/test": "^1.0",
    "msbios/debug": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "alter": "./vendor/bin/doctrine-module orm:schema-tool:update --force",
    "themes": [
      "cd public && ln -s ../vendor/msbios/cpanel/themes/limitless/public/ limitless",
      "cd public && ln -s ../vendor/msbios/application/themes/default/public/ default"
    ],
    "serve": "php -S 0.0.0.0:3107 -t public public/index.php",
    "serve-ios": "php -S 0.0.0.0:3107 -t public index.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\CPanel\\Doctrine"
    }
  }
}
